class Gauss{
	void main(){
		int start;
		int end;
		int result;
		start = 1;
		end = 10000;
		result = sum(start,end);//调用求和方法sum，计算1加到100的和
		printf("%d加到%d的和是%d",start,end,result);
	}
	
	int sum(int start,int end){
		printf("起始数字是：%d,结束数字是：%d\n",start,end);
		return (start + end ) * (end - start + 1) / 2;
	}
}