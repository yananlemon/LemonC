// 打印九九乘法表
class MulTable{
	void main(){
		int k;
		k = showMulTable();
		printLine();
		showMulTable();
	}
	
	int showMulTable(){
		int k;
		int t;
		k = 1;
		t = 1;
		//while(k < 10 ){
		while(k < 2*5 ){
			t = 1;
			while(t < 10 ){				
				printf("%d*%d=%d\t",k,t,k*t);
				t = t + 1;
			}
			k = k+1;
			printLine();
		}
		return 0;
	}
}